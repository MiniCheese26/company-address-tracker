{
  "name": "company-address-tracker",
  "version": "1.0.1",
  "description": "Here to track addresses",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "win-start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts .",
    "rebuild": "electron-rebuild",
    "rebuild-sqlite3": "cd node_modules/better-sqlite3 && node-gyp rebuild --target=13.1.6 --arch=x64 --dist-url=https://electronjs.org/headers"
  },
  "os": [
    "win32",
    "linux"
  ],
  "keywords": [],
  "author": "Emily Medhurst",
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "company-address-tracker",
            "authors": "Emily Medhurst"
          }
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer.ts",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ],
      "publishers": [
        {
          "name": "@electron-forge/publisher-s3",
          "platforms": [
            "win32"
          ],
          "config": {
            "bucket": "companyaddresstrackerpseudo",
            "public": true,
            "secretAccessKey": "sQgGZ1lCZDbGEmAfqkqwjLMhy7blSrMOuQ1aG7ht",
            "accessKeyId": "AKIAQWXRPHKDXS3VME3A",
            "folder": "RELEASES"
          }
        }
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.57",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.57",
    "@electron-forge/plugin-webpack": "6.0.0-beta.57",
    "@electron-forge/publisher-github": "^6.0.0-beta.57",
    "@electron-forge/publisher-s3": "^6.0.0-beta.57",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@styled-icons/boxicons-regular": "^10.34.0",
    "@styled-icons/entypo": "^10.34.0",
    "@styled-icons/evaicons-outline": "^10.34.0",
    "@styled-icons/fa-regular": "^10.34.0",
    "@styled-icons/material": "^10.34.0",
    "@types/node": "^16.0.1",
    "@types/pg": "^8.6.1",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "@types/styled-components": "^5.1.11",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.28.2",
    "electron": "13.1.6",
    "electron-context-menu": "^3.1.1",
    "electron-fetch": "^1.7.3",
    "electron-rebuild": "^2.3.5",
    "eslint": "^7.6.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.4",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^6.0.1",
    "nanoid": "^3.1.23",
    "node-loader": "^2.0.0",
    "pg": "^8.6.0",
    "pg-native": "^3.0.0",
    "styled-components": "^5.3.0",
    "ts-loader": "^9.2.2",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
